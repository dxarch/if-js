{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prefer-const": "error",
    "comma-dangle": ["error", "always-multiline"],
    "semi": ["error", "always"],
    "no-unused-vars": "off"
  }
}
